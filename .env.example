# Grenmetey Investments Environment Variables

# AI Service Configuration
# Replace with your actual API key from your chosen AI provider
VITE_AI_API_KEY=your_openai_api_key_here

# API Endpoints
VITE_API_BASE_URL=http://localhost:3001/api
VITE_AI_SERVICE_URL=https://api.openai.com/v1

# Feature Flags
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_SMART_SEARCH=true
VITE_ENABLE_JSON_PROMPTS=true

# Application Settings
VITE_APP_NAME=Grenmetey Investments
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LOCATION=South Africa

# Debug Settings (development only)
VITE_DEBUG_AI_RESPONSES=false
VITE_MOCK_AI_RESPONSES=true
